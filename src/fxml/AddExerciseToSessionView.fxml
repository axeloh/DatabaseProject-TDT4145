<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AddExerciseController">
   <children>
      <Pane prefHeight="600.0" prefWidth="800.0">
         <children> 
            <Text layoutX="306.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Øvelse">
               <font>
                  <Font size="64.0" />
               </font>
            </Text>
            <ComboBox fx:id="cmbExercises" layoutX="325.0" layoutY="139.0" prefWidth="150.0" promptText="Velg øvelse..." />
            <TextField fx:id="comment" layoutX="326.0" layoutY="194.0" promptText="Kommentar/resultat" />
            <Button fx:id="btnAnotherOne" layoutX="337.0" layoutY="251.0" mnemonicParsing="false" onAction="#anotherExercise" text="Another one (øvelse)" />
            <Button fx:id="btnDone" layoutX="347.0" layoutY="315.0" mnemonicParsing="false" onAction="#sendInSession" prefHeight="66.0" prefWidth="107.0" text="Ferdig" />
            <Button fx:id="btnCancel" layoutX="368.0" layoutY="407.0" mnemonicParsing="false" onAction="#cancel" text="Avbryt" />
         </children>
      </Pane>
   </children>
</AnchorPane>
